{
  "name": "generator-boilerplate",
  "version": "1.1.2",
  "description": "Yeoman generator to generate boilerplate template",
  "homepage": "https://github.com/dheeraj-rn/generator-boilerplate",
  "author": {
    "name": "Dheeraj",
    "url": "https://github.com/dheeraj-rn/"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "yeoman-generator",
    "yeoman",
    "generator",
    "create",
    "scaffold",
    "boilerplate",
    "server",
    "hapi",
    "hpal"
  ],
  "devDependencies": {
    "coveralls": "^3.0.11",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-config-xo": "^0.29.1",
    "eslint-plugin-prettier": "^3.1.3",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "lint-staged": "^10.1.3",
    "prettier": "^2.0.4",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^2.4.1"
  },
  "engines": {
    "npm": ">= 8.0.0"
  },
  "dependencies": {
    "chalk": "^4.0.0",
    "yeoman-generator": "^4.8.2",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest"
  },
  "repository": "dheeraj-rn/generator-boilerplate",
  "license": "MIT"
}
